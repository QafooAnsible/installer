{
    "name": "monofone/ansible-installer",
    "type": "composer-installer",
    "license": "MIT",
    "description": "Installer for ansible roles",
    "keywords": [
        "installer",
        "ansible"
    ],
    "homepage": "http://github.com/monofone/ansible-installer/",
    "authors": [
        {
            "name": "Kyle Robinson Young",
            "email": "kyle@dontkry.com",
            "homepage": "https://github.com/shama"
        }
    ],
    "autoload": {
        "psr-0": { "Composer\\Installers\\": "src/" }
    },
    "extra": {
        "class": "Composer\\Installers\\Installer",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpunit/phpunit": "3.7.*"
    }
}
